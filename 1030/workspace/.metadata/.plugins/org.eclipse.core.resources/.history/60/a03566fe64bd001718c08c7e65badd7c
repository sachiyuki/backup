
public class SmartPhone implements Mp3Player, NewFunction {

	@Override
	public void play() {
		System.out.println("再生");

	}

	@Override
	public void stop() {
		System.out.println("停止");

	}

	@Override
	public void next() {
		System.out.println("次へ");

	}

	@Override
	public void back() {
		System.out.println("戻る");

	}

	@Override
	public void call() {
		System.out.println("");

	}

	@Override
	public void mail() {
		System.out.println("");

	}

	@Override
	public void photo() {
		System.out.println("");

	}

	@Override
	public void internet() {
		System.out.println();

	}

}
